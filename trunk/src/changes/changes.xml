<document xmlns="http://maven.apache.org/changes/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/changes/1.0.0 http://maven.apache.org/xsd/changes-1.0.0.xsd">
	<properties>
		<title>Freemind Search Plugin</title>
		<author email="sourceforge@leonarduk.com">Stephen Leonard</author>
	</properties>
	<body>
		<release version="0.2" date="2014-04-01" description="First set of GUI improvements.">
			<action type="add" dev="Stephen Leonard" issue="3">Remove Score
				from text panel when click on result
			</action>
			<action type="add" dev="Stephen Leonard" issue="6">Clicking return
				from search box should activate "search" button
			</action>
			<action type="add" dev="Stephen Leonard" issue="7">Allow editing
				of selected folder field
			</action>
			<action type="add" dev="Stephen Leonard" issue="9">Plugin should
				remember state
			</action>

			<action type="add" dev="Stephen Leonard" issue="13">Use maven
				project version in plugin config file
			</action>
		</release>
		<release version="0.1" date="2014-02-17"
			description="First cut. Integrates with FreeMind.">
			<action type="add" dev="Stephen Leonard">Implemented minimum working plugin.
			</action>
			<action type="add" dev="Stephen Leonard">Created Maven repository</action>
		</release>
	</body>
</document>